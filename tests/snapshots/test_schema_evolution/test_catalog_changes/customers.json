{
  "tap_stream_id": "customers",
  "replication_key": "modifiedOn",
  "replication_method": "INCREMENTAL",
  "key_properties": [
    "id"
  ],
  "schema": {
    "properties": {
      "id": {
        "description": "ID of the customer",
        "format": "int64",
        "type": "integer"
      },
      "active": {
        "description": "False indicates that someone has deactivated the customer record, typically upon merging with another record.",
        "type": "boolean"
      },
      "name": {
        "description": "Name of the customer",
        "type": "string"
      },
      "type": {
        "description": "Indicates a customer's type.",
        "type": "string",
        "enum": [
          "Residential",
          "Commercial",
          null
        ]
      },
      "address": {
        "properties": {
          "street": {
            "description": "Street",
            "type": "string"
          },
          "unit": {
            "description": "Unit",
            "type": [
              "string",
              "null"
            ]
          },
          "city": {
            "description": "City",
            "type": "string"
          },
          "state": {
            "description": "State",
            "type": "string"
          },
          "zip": {
            "description": "Zip",
            "type": "string"
          },
          "country": {
            "description": "Country",
            "type": "string"
          },
          "latitude": {
            "description": "Latitude",
            "format": "double",
            "type": [
              "number",
              "null"
            ]
          },
          "longitude": {
            "description": "Longitude",
            "format": "double",
            "type": [
              "number",
              "null"
            ]
          }
        },
        "description": "Bill-To address of the customer record",
        "type": "object",
        "required": [
          "street",
          "city",
          "state",
          "zip",
          "country"
        ],
        "additionalProperties": false
      },
      "customFields": {
        "items": {
          "properties": {
            "typeId": {
              "description": "ID of the custom field",
              "format": "int64",
              "type": "integer"
            },
            "name": {
              "description": "Name/label of the custom field",
              "type": [
                "string",
                "null"
              ]
            },
            "value": {
              "description": "Value of the custom field",
              "type": [
                "string",
                "null"
              ]
            }
          },
          "type": "object",
          "required": [
            "typeId"
          ],
          "additionalProperties": false
        },
        "description": "Customer record\u2019s custom fields",
        "type": "array"
      },
      "balance": {
        "description": "Customer\u2019s account balance",
        "format": "decimal",
        "type": "number"
      },
      "taxExempt": {
        "description": "Tax Exempt status of the customer.",
        "type": "boolean"
      },
      "tagTypeIds": {
        "items": {
          "format": "int64",
          "type": "integer"
        },
        "description": "Tag Type IDs associated with the Customer",
        "type": "array"
      },
      "doNotMail": {
        "description": "Customer has been flagged as \u201cdo not mail\u201d",
        "type": "boolean"
      },
      "doNotService": {
        "description": "Customer has been flagged as \u201cdo not service\u201d",
        "type": "boolean"
      },
      "createdOn": {
        "description": "DateTime (UTC) that customer record was created",
        "format": "date-time",
        "type": "string"
      },
      "createdById": {
        "description": "User ID who created the record.",
        "format": "int64",
        "type": "integer"
      },
      "modifiedOn": {
        "description": "Modified on (UTC) for the record.",
        "format": "date-time",
        "type": "string"
      },
      "mergedToId": {
        "description": "The customer ID of the record that this record was previously merged to.",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "externalData": {
        "items": {
          "properties": {
            "key": {
              "description": "External data key.",
              "type": "string"
            },
            "value": {
              "description": "External data value.",
              "type": "string"
            }
          },
          "type": "object",
          "required": [
            "key",
            "value"
          ],
          "additionalProperties": false
        },
        "description": "List of external data attached to this Customer,\nwhich corresponds to the application GUID provided in the request.",
        "type": "array"
      }
    },
    "type": "object",
    "required": [
      "id",
      "active",
      "name",
      "type",
      "address",
      "customFields",
      "balance",
      "taxExempt",
      "tagTypeIds",
      "doNotMail",
      "doNotService",
      "createdOn",
      "createdById",
      "modifiedOn",
      "externalData"
    ],
    "additionalProperties": false
  },
