  "key_properties": [
    "id"
  ],
  "schema": {
    "properties": {
      "id": {
        "description": "The ID of the call",
        "format": "int64",
        "type": "integer"
      },
      "duration": {
        "description": "The duration of the call",
        "format": "duration",
        "type": "string"
      },
      "from": {
        "description": "The caller's phone",
        "type": "string"
      },
      "to": {
        "description": "The called party's phone",
        "type": "string"
      },
      "direction": {
        "description": "Indicates the direction of a call",
        "type": "string"
      },
      "status": {
        "description": "Indicates the status of a call",
        "type": "string"
      },
      "type": {
        "description": "Specifies the type (classification) of the call. If null, the call was not classified yet. Also,\n            outbound calls don't have classification and CallType is always null for outbound calls",
        "type": [
          "string",
          "null"
        ]
      },
      "recordingUrl": {
        "description": "The URL for call recording. Can be null if the call hasn't a recording",
        "type": [
          "string",
          "null"
        ]
      },
      "voiceMailPath": {
        "description": "The URL for call voicemail. Can be null if the call hasn't a voicemail. This field may have value only for\nPhones Pro tenants",
        "type": [
          "string",
          "null"
        ]
      },
      "createdOn": {
        "description": "The time the call was created (i.e. inbound call was received or outbound call was initiated)\n            ",
        "format": "date-time",
        "type": "string"
      },
      "modifiedOn": {
        "description": "The last time the call properties were updated",
        "format": "date-time",
        "type": "string"
      },
      "reason": {
        "properties": {
          "id": {
            "description": "The ID of the entity",
            "format": "int64",
            "type": "integer"
          },
          "name": {
            "description": "The name of the entity",
            "type": "string"
          }
        },
        "description": "The info about the call reason linked to the call",
        "type": [
          "object",
          "null"
        ],
        "required": [
          "id",
          "name"
        ],
        "additionalProperties": false
      },
      "customer": {
        "properties": {
          "id": {
            "description": "The ID of the entity",
            "format": "int64",
            "type": "integer"
          },
          "name": {
            "description": "The name of the entity",
            "type": "string"
          }
        },
        "description": "The info about the customer linked to the call",
        "type": [
          "object",
          "null"
        ],
        "required": [
          "id",
          "name"
        ],
        "additionalProperties": false
      },
      "location": {
        "properties": {
          "id": {
            "description": "The ID of the entity",
            "format": "int64",
            "type": "integer"
          },
          "name": {
            "description": "The name of the entity",
            "type": "string"
          }
        },
        "description": "The info about the location linked to the call",
        "type": [
          "object",
          "null"
        ],
        "required": [
          "id",
          "name"
        ],
        "additionalProperties": false
      },
      "campaign": {
        "properties": {
          "id": {
            "description": "The ID of the entity",
            "format": "int64",
            "type": "integer"
          },
          "name": {
            "description": "The name of the entity",
            "type": "string"
          }
        },
        "description": "The info about the phone advertising campaign linked to the call",
        "type": [
          "object",
          "null"
        ],
        "required": [
          "id",
          "name"
        ],
        "additionalProperties": false
      },
      "job": {
        "properties": {
          "id": {
            "description": "The ID of the job",
            "format": "int64",
            "type": "integer"
          },
          "number": {
            "description": "Human readable number of the job",
            "type": "string"
          }
        },
        "description": "The info about the job linked to the call",
        "type": [
          "object",
          "null"
        ],
        "required": [
          "id",
          "number"
        ],
        "additionalProperties": false
      },
      "agent": {
        "properties": {
          "id": {
            "description": "The ID of the entity",
            "format": "int64",
            "type": "integer"
          },
          "name": {
            "description": "The name of the entity",
            "type": "string"
          }
        },
        "description": "The info about the user agent who was assigned to the call. For inbound calls this is the user who\n            clicked the call bubble last time, or outbound calls this is a user who initiated the call. For Phones Pro,\n            this may differ to CreatedBy for outbound calls if the outbound call was transferred between users\n            ",
        "type": [
          "object",
          "null"
        ],
        "required": [
          "id",
          "name"
        ],
        "additionalProperties": false
      },
      "createdBy": {
        "properties": {
          "id": {
            "description": "The ID of the entity",
            "format": "int64",
            "type": "integer"
          },
          "name": {
            "description": "The name of the entity",
            "type": "string"
          }
        },
        "description": "The info about the user who created the outbound call",
        "type": [
          "object",
          "null"
        ],
        "required": [
          "id",
          "name"
        ],
        "additionalProperties": false
      },
      "excuseMemo": {
