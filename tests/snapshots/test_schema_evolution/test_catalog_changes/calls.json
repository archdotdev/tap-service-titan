        "type": [
          "string",
          "null"
        ]
      },
      "tag": {
        "description": "Call Tag",
        "type": [
          "string",
          "null"
        ]
      },
      "active": {
        "description": "Gets or sets whether the entity is active.",
        "type": "boolean"
      },
      "lead": {
        "properties": {
          "id": {
            "description": "The ID of the entity",
            "format": "int64",
            "type": "integer"
          }
