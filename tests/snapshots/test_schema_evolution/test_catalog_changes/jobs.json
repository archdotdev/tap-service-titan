{
  "tap_stream_id": "jobs",
  "replication_key": "modifiedOn",
  "replication_method": "INCREMENTAL",
  "key_properties": [
    "id"
  ],
  "schema": {
    "properties": {
      "summary": {
        "description": "Job summary",
        "format": "html",
        "type": [
          "string",
          "null"
        ]
      },
      "customFields": {
        "items": {
          "properties": {
            "typeId": {
              "format": "int64",
              "type": "integer"
            },
            "name": {
              "type": "string"
            },
            "value": {
              "type": [
                "string",
                "null"
              ]
            }
          },
          "type": "object",
          "required": [
            "typeId",
            "name",
            "value"
          ],
          "additionalProperties": false
        },
        "description": "Custom fields on the job",
        "type": "array"
      },
      "externalData": {
        "items": {
          "properties": {
            "key": {
              "description": "External data key.",
              "type": "string"
            },
            "value": {
              "description": "External data value.",
              "type": "string"
            }
          },
          "type": "object",
          "required": [
            "key",
            "value"
          ],
          "additionalProperties": false
        },
        "description": "List of external data attached to this job,\nthat corresponds to the application guid provided in the request.",
        "type": [
          "array",
          "null"
        ]
      },
      "id": {
        "description": "ID of the job",
        "format": "int64",
        "type": "integer"
      },
      "jobNumber": {
        "description": "Job number",
        "type": "string"
      },
      "projectId": {
        "description": "ID of the job's project",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "customerId": {
        "description": "ID of the job's customer",
        "format": "int64",
        "type": "integer"
      },
      "locationId": {
        "description": "ID of the job's location",
        "format": "int64",
        "type": "integer"
      },
      "jobStatus": {
        "description": "Status of the job",
        "type": "string"
      },
      "completedOn": {
        "description": "Date/time (in UTC) when the job was completed",
        "format": "date-time",
        "type": [
          "string",
          "null"
        ]
      },
      "businessUnitId": {
        "description": "ID of the job's business unit",
        "format": "int64",
        "type": "integer"
      },
      "jobTypeId": {
        "description": "ID of job type",
        "format": "int64",
        "type": "integer"
      },
      "priority": {
        "description": "Priority of the job",
        "type": "string"
      },
      "campaignId": {
        "description": "ID of the job's campaign",
        "format": "int64",
        "type": "integer"
      },
      "appointmentCount": {
        "description": "Number of appointments on the job",
        "format": "int64",
        "type": "integer"
      },
      "firstAppointmentId": {
        "description": "ID of the first appointment on the job",
        "format": "int64",
        "type": "integer"
      },
      "lastAppointmentId": {
        "description": "ID of the last appointment on the job",
        "format": "int64",
        "type": "integer"
      },
      "recallForId": {
        "description": "ID of the job for which this job is a recall",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "warrantyId": {
        "description": "ID of the job for which this job is a warranty",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "jobGeneratedLeadSource": {
        "properties": {
          "jobId": {
            "format": "int64",
            "type": [
              "integer",
              "null"
            ]
          },
          "employeeId": {
            "format": "int64",
            "type": [
              "integer",
              "null"
            ]
          }
        },
        "description": "Object that contains:\n            JobId: ID of the job from which this job was generated\n            EmployeeId: ID of the office user or technician\n            ",
        "type": [
          "object",
          "null"
        ],
        "additionalProperties": false
      },
      "noCharge": {
        "description": "Whether the job is a no-charge job",
        "type": "boolean"
      },
      "notificationsEnabled": {
        "description": "Whether notifications will be sent to customers",
        "type": "boolean"
      },
      "createdOn": {
        "description": "Date/time (in UTC) when the job was created",
        "format": "date-time",
        "type": "string"
      },
      "createdById": {
        "description": "ID of the user who created the job",
        "format": "int64",
        "type": "integer"
      },
      "modifiedOn": {
        "description": "Date/time (in UTC) when job was last modified",
        "format": "date-time",
        "type": "string"
      },
      "tagTypeIds": {
        "items": {
          "format": "int64",
          "type": "integer"
        },
        "description": "Tags on the job",
        "type": "array"
      },
      "leadCallId": {
        "description": "ID of the Call that resulted in this job",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "partnerLeadCallId": {
        "description": "ID of the Call that 3rd-party agents are enabled to set.",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "bookingId": {
        "description": "ID of the Booking that resulted in this job",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "soldById": {
        "description": "ID of the Technician that sold this job",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "customerPo": {
        "description": "Customer PO",
        "type": [
          "string",
          "null"
        ]
      },
      "invoiceId": {
        "description": "ID of the invoice associated with this job.",
        "format": "int64",
        "type": "integer"
      },
      "membershipId": {
        "description": "ID of the Membership associated with this job.",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "total": {
        "description": "Total amount of the job",
        "format": "decimal",
        "type": [
          "number",
          "null"
        ]
      },
      "active": {
        "description": "Whether this job is active, or not anymore.",
        "type": "boolean"
      }
    },
    "type": "object",
    "required": [
      "customFields",
      "externalData",
      "id",
      "jobNumber",
      "customerId",
      "locationId",
      "jobStatus",
      "businessUnitId",
      "jobTypeId",
      "priority",
      "campaignId",
      "appointmentCount",
      "firstAppointmentId",
      "lastAppointmentId",
      "noCharge",
      "notificationsEnabled",
      "createdOn",
      "createdById",
      "modifiedOn",
      "tagTypeIds",
      "customerPo",
      "invoiceId",
      "active"
    ],
    "additionalProperties": false
  },
