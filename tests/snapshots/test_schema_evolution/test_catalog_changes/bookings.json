{
  "tap_stream_id": "bookings",
  "replication_key": "modifiedOn",
  "replication_method": "INCREMENTAL",
  "key_properties": [
    "id"
  ],
  "schema": {
    "properties": {
      "summary": {
        "description": "Booking summary",
        "type": [
          "string",
          "null"
        ]
      },
      "id": {
        "description": "ID of the booking",
        "format": "int64",
        "type": "integer"
      },
      "source": {
        "description": "Source of the booking",
        "type": "string"
      },
      "createdOn": {
        "description": "when the booking was created",
        "format": "date-time",
        "type": "string"
      },
      "name": {
        "description": "Name of the booking's customer",
        "type": "string"
      },
      "address": {
        "properties": {
          "street": {
            "description": "Booking address street",
            "type": [
              "string",
              "null"
            ]
          },
          "unit": {
            "description": "Booking address unit",
            "type": [
              "string",
              "null"
            ]
          },
          "city": {
            "description": "Booking address city",
            "type": [
              "string",
              "null"
            ]
          },
          "state": {
            "description": "Booking address state",
            "type": [
              "string",
              "null"
            ]
          },
          "zip": {
            "description": "Booking address zip code",
            "type": [
              "string",
              "null"
            ]
          },
          "country": {
            "description": "Booking address country",
            "type": [
              "string",
              "null"
            ]
          }
        },
        "description": "Address of the booking",
        "type": [
          "object",
          "null"
        ],
        "required": [
          "street",
          "city",
          "state",
          "zip",
          "country"
        ],
        "additionalProperties": false
      },
      "customerType": {
        "description": "Indicates a customer's type.",
        "type": [
          "string",
          "null"
        ],
        "enum": [
          "Residential",
          "Commercial",
          null
        ]
      },
      "start": {
        "description": "Booking's start date time, in UTC",
        "format": "date-time",
        "type": "string"
      },
      "campaignId": {
        "description": "ID of the booking's campaign",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "businessUnitId": {
        "description": "ID of the booking's business unit",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "isFirstTimeClient": {
        "description": "Returns true if first time client",
        "type": [
          "boolean",
          "null"
        ]
      },
      "uploadedImages": {
        "items": {
          "type": "string"
        },
        "description": "Uploaded images",
        "type": [
          "array",
          "null"
        ]
      },
      "isSendConfirmationEmail": {
        "description": "Returns true if customer should receive email when booking is created",
        "type": [
          "boolean",
          "null"
        ]
      },
      "status": {
        "description": "",
        "type": "string",
        "enum": [
          "New",
          "Converted",
          "Dismissed",
          "Accepted"
        ]
      },
      "dismissingReasonId": {
        "description": "ID of the reason booking was dismissed",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "jobId": {
        "description": "ID of the job booked from the booking",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "externalId": {
        "description": "External ID of booking",
        "type": "string"
      },
      "priority": {
        "description": "",
        "type": [
          "string",
          "null"
        ],
        "enum": [
          "Low",
          "Normal",
          "High",
          "Urgent"
        ]
      },
      "jobTypeId": {
        "description": "ID of the booking's job type",
        "format": "int64",
        "type": [
          "integer",
          "null"
        ]
      },
      "bookingProviderId": {
        "description": "Booking provider id",
        "format": "int64",
        "type": "integer"
      },
      "modifiedOn": {
        "description": "When the booking was modified",
        "format": "date-time",
        "type": "string"
      },
      "active": {
        "type": "boolean"
      }
    },
    "type": "object",
    "required": [
      "id",
      "source",
      "createdOn",
      "name",
      "start",
      "status",
      "externalId",
      "bookingProviderId",
      "modifiedOn",
      "active"
    ],
    "additionalProperties": false
  },
